ALTER TABLE -CODETABLE- RENAME TO -CODETABLE-_PRE_1_99_08;

CREATE TABLE -CODETABLE- (
        name VARCHAR2(255) primary key,
        plsql_spec VARCHAR2(255),
        language VARCHAR2(16),
        code CLOB
);

INSERT INTO -CODETABLE- (name, language, code)
   SELECT name, language, code from -CODETABLE-_PRE_1_99_08;
